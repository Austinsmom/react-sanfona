{
  "name": "react-sanfona",
  "version": "0.0.1",
  "description": "React accordion component",
  "main": "./dist-modules/index.js",
  "scripts": {
    "bundle-demo": "webpack --config webpack.config.demo.js",
    "bundle-dist": "webpack --config webpack.config.dist.js",
    "bundle-modules": "trash dist-modules/* --force && babel src --loose --out-dir dist-modules",
    "demo": "webpack-dev-server --config webpack.config.demo.js",
    "lint": "eslint ./src",
    "test": "npm run lint && mocha -R spec --compilers jsx:test/compiler.js ./src/**/test.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daviferreira/react-sanfona.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "accordion",
    "accessibility",
    "aria",
    "widget"
  ],
  "author": "Davi Ferreira <hi@daviferreira.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daviferreira/react-sanfona/issues"
  },
  "homepage": "https://github.com/daviferreira/react-sanfona",
  "peerDependencies": {
    "react": "0.13.x"
  },
  "devDependencies": {
    "autoprefixer-loader": "2.0.0",
    "babel": "5.4.7",
    "babel-core": "5.4.7",
    "babel-loader": "5.1.3",
    "classnames": "2.1.2",
    "css-loader": "0.14.4",
    "eslint": "0.22.1",
    "eslint-plugin-react": "2.5.0",
    "jsdom": "3.1.2",
    "mocha": "2.2.5",
    "mocha-jsdom": "0.4.0",
    "node-sass": "3.1.2",
    "normalize.css": "3.0.3",
    "sass-loader": "1.0.2",
    "sinon": "1.14.1",
    "style-loader": "0.12.3",
    "trash": "1.4.1",
    "unexpected": "7.5.1",
    "unexpected-sinon": "6.1.2",
    "uuid": "2.0.1",
    "webpack": "1.9.10",
    "webpack-dev-server": "1.9.0"
  }
}
